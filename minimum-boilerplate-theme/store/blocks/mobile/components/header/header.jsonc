{
  "sticky-layout#mobile": {
    "title": "Sticky header container on mobile",
    "children": [
      "flex-layout.row#mobile_search",
      "flex-layout.row#mobile_menu"
    ],
    "props": {
      "blockClass": "sticky-header-mobile"
    }
  },
  "flex-layout.row#mobile_search": {
    "title": "First header row container on mobile",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "marginTop": 2,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": "search_header_mobile--text"
    },
    "children": ["header-row#1-mobile"]
  },
  "flex-layout.row#mobile_menu": {
    "title": "Second header row container on mobile",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "blockClass": "menu_header_desktop--text"
    },
    "children": ["header-row#2-mobile"]
  },
  "header-row#1-mobile": {
    "title": "First header row child on mobile",
    "children": [
      "flex-layout.row#1-mobile",
      "logo",
      "flex-layout.col#spacer",
      "flex-layout.col#search_container",
      "minicart.v2",
      "login"
    ],
    "props": {
      "blockClass": "header_one_row_mobile--text"
    }
  },
  "header-row#2-mobile": {
    "title": "Second header row child on mobile",
    "children": [
      "vtex.menu@2.x:menu#menu-categories"
    ],
    "props": {
      "blockClass": "header_two_row_mobile--text"
    }
  },
  "flex-layout.col#search_container": {
    "title": "MagnifyingGlas icon container mobile",
    "children": ["icon#search-mobile"],
    "props": {
      "preventVerticalStretch": true,
      "blockClass": "search_icon_row--text"
    }
  },
  "icon#search-mobile": {
    "title": "Icon search mobile",
    "props": {
      "id": "MagnifyingGlas",
      "weight": "bold",
      "viewBox": "0 0 24 24",
      "size": 24,
      "height": "18",
      "width": "18"
    }
  }
}
