          "horizontalAlign": "center",
          "borderWidth": 0,
          "boder": "all",
          "colJustify": "around",
          "marginTop": 8,
          "colGap": 5,
          "colSizing": "auto",
          "rowGap": 0,
          "preventVerticalStretch": true
