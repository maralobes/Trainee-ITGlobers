{
  "slider-layout#computer__carousel": {
      "title": "Computer carousel",
      "props": {
        "itemsPerPage": {
          "desktop": 1,
          "tablet": 1,
          "phone": 1
        },
        "infinite": false,
        "showNavigationArrows": "desktopOnly",
        "blockClass": "second-carousel--text"
      },
      "children": [
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home",
        "flex-layout.row#computer__carousel--second-slider-home"
      ]
    },

    "flex-layout.row#computer__carousel--second-slider-home": {
      "title": "Second slider container",
      "children":[
        "flex-layout.col#second__carousel--second-slider-home",
        "flex-layout.col#second__carousel--second-slider-home",
        "flex-layout.col#second__carousel--second-slider-home",
        "flex-layout.col#second__carousel--second-slider-home",
        "flex-layout.col#second__carousel--second-slider-home"
      ],
      "props":{
        "borderWidth": 0,
        "boder": "all",
        "colJustify": "around",
        "marginTop": 8,
        "colGap": 5,
        "colSizing": "auto",
        "rowGap": 0,
        "preventVerticalStretch": true
      }
    },

    "flex-layout.col#second__carousel--second-slider-home": {
      "title": "slider col second carousel",
      "children": [
        "flex-layout.row#computer__image--second-slider-home",
